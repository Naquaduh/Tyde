{
  "name": "Tyde",
  "version": "0.1.0",
  "description": "Tyde Angular Client",
  "scripts": {
    "build": "rimraf dist && cross-env NODE_ENV=prod webpack --bail --progress --profile",
    "profile": "cross-env NODE_ENV=dev webpack --debug --output-pathinfo --progress --profile --json > stats.json",
    "dev-server": "cross-env NODE_ENV=dev webpack-dev-server --hot --inline --debug --output-pathinfo --progress --profile --colors --compress --history-api-fallback",
    "server": "npm run build && serve dist/ --port 8080 --compress",
    "test": "cross-env NODE_ENV=test karma start",
    "test-watch": "cross-env NODE_ENV=test karma start --auto-watch --no-single-run",
    "start": "npm run dev-server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Naquaduh/Tyde"
  },
  "author": "Jason DiMeo",
  "license": "MIT",
  "homepage": "",
  "dependencies": {
    "angular": "^1.5.7",
    "angular-ui-bootstrap": "^1.3.3",
    "angular-ui-router": "^1.0.0-beta.1",
    "bootstrap": "^3.3.6",
    "lodash": "^4.13.1",
    "oclazyload": "^1.0.9"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.7",
    "autoprefixer": "^6.3.7",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "bootstrap-sass": "^3.3.6",
    "copy-webpack-plugin": "^3.0.1",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.22.0",
    "image-webpack-loader": "^2.0.0",
    "jasmine-core": "^2.3.4",
    "karma": "^1.1.1",
    "karma-coverage": "^1.1.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "ng-annotate-loader": "^0.1.0",
    "node-libs-browser": "^1.0.0",
    "node-sass": "^3.8.0",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.4",
    "postcss-loader": "^0.9.1",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^1.6.0",
    "rimraf": "^2.5.3",
    "sass-loader": "^4.0.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-config": "^4.0.0",
    "webpack-dev-server": "^1.14.1"
  }
}
